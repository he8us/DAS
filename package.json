        "babel-preset-react": "^6.11.1",
        "babel-preset-react-hmre": "^1.1.1",
        "babel-preset-survivejs-kanban": "^0.3.3",
        "chai": "^3.5.0",
        "chai-as-promised": "^5.3.0",
        "chai-enzyme": "^0.5.0",
        "cross-env": "^2.0.0",
        "css-loader": "^0.23.1",
        "enzyme": "^2.4.1",
        "eslint": "^3.2.2",
